{
    "useGoldMentions": false,
    "algorithm": "hcoref",
    "scaffoldingSieves": [
        {
            "name": "proper",
            "sentenceLimit": 15,
            "threshold": 0.3,
            "mentionPos": "PROPN",
            "antecedentPos": "PROPN"
        },
        {
            "name": "common",
            "sentenceLimit": 15,
            "threshold": 0.3,
            "mentionPos": "NOUN",
            "antecedentPos": "NOUN"
        },
        {
            "name": "pronoun",
            "sentenceLimit": 15,
            "threshold": 0.2,
            "mentionPos": "PRON",
            "antecedentPos": "ANY"
        }
    ],
    "inputFile": "./data/textinator/devfromtrain.json",
    "trainingInputFile": "./data/textinator/train.json",
    "wordVectorFile": "../model.bin",
    "maxDepth": -1,
    "useAllDocs": true,
    "docId": 0,
    "compareClusters": false,
    "writeForScoring": true,
    "debugMentionDetection": false,
    "debugFeatureSelection": true,
    "minimalMutualInformation": 0.0001,
    "allowedFeatureRarity": 30,
    "useSubsampling": true
}